{
    "name": "dungeonworx/laravel-devicedetector",
    "description": "Integration with DeviceDetector and Laravel.",
    "type": "library",
    "homepage": "https://dungeonworx.org/devicedetector/",
    "keywords": [
        "laravel",
        "laravel-facade",
        "laravel-middleware",
        "laravel-package",
        "user-agent",
        "user-agent-parser"
    ],
    "prefer-stable": true,
    "license": "MIT",
    "authors": [
        {
            "name": "Steven Crothers",
            "email": "steven.crothers@gmail.com",
            "homepage": "https://github.com/scrothers",
            "role": "owner"
        }
    ],
    "support": {
        "issues": "https://github.com/dungeonworx/devicedetector/issues",
        "wiki": "https://github.com/dungeonworx/devicedetector/wiki",
        "source": "https://github.com/dungeonworx/devicedetector",
        "docs": "https://dungeonworx.org/devicedetector/",
        "rss": "https://github.com/dungeonworx/devicedetector/releases.atom"
    },
    "autoload": {
        "psr-4": {
            "Dungeonworx\\DeviceDetector\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Dungeonworx\\DeviceDetector\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Dungeonworx\\DeviceDetector\\DeviceDetectorServiceProvider"
            ],
            "aliases": {
                "DeviceDetector": "Dungeonworx\\DeviceDetector\\DeviceDetector"
            }
        }
    },
    "require": {
        "piwik/device-detector": "^3.11",
        "illuminate/support": "^5.7",
        "illuminate/contracts": "^5.7",
        "illuminate/http": "^5.7"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.5"
    }
}
