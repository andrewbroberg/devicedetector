{
    "name": "dungeonworx/laravel-devicedetector",
    "description": "Integration with DeviceDetector and Laravel.",
    "type": "library",
    "keywords": [
        "user-agent",
        "laravel"
    ],
    "prefer-stable": true,
    "license": "MIT",
    "authors": [
        {
            "name": "Steven Crothers",
            "email": "steven.crothers@gmail.com",
            "homepage": "https://github.com/scrothers",
            "role": "owner"
        }
    ],
    "support": {
        "issues": "https://github.com/dungeonworx/devicedetector/issues",
        "wiki": "https://github.com/dungeonworx/devicedetector/wiki",
        "source": "https://github.com/dungeonworx/devicedetector"
    },
    "autoload": {
        "psr-4": {
            "Gameworx\\DeviceDetector\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Gameworx\\DeviceDetector\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Gameworx\\DeviceDetector\\DeviceDetectorServiceProvider"
            ],
            "aliases": {
                "DeviceDetector": "Gameworx\\DeviceDetector\\DeviceDetector"
            }
        }
    },
    "require": {
        "piwik/device-detector": "^3.11",
        "illuminate/support": "^5.7",
        "illuminate/contracts": "^5.7",
        "illuminate/http": "^5.7"
    },
    "require-dev": {
        "laravel/framework": "^5.7",
        "phpunit/phpunit": "^7.5"
    }
}
